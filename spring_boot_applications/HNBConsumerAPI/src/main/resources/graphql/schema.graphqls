    ownerDesignation: String
    ownerDesignationTitle: String
    originatingBankNumber: String
    signerPresentAtAccountOpeningIndicator: Boolean
    effectiveStartDate: String
    effectiveEndDate: String
    accountId: String
    externalAccountId: String
    accountShortNickname: String
    customerRelationshipsCustomerId: String

}

