type Query {
    getCustomerAccountInfo(customerId: String!): CustomerAccountInfo
    getTransactionHistory(accountId: String!): TransactionHistory
    getCustomerLatestTransaction(customerId: String!): CustomerLatestTransaction
    getLatestTransactionForCustomer(customerId: String!): LatestTransactionRecord
    getAggregatedTransactions(accountId: String!, startDate: String!, endDate: String!): AggregatedTransactions
    getPast2DayTransactionsByAccountId(accountId: String!): Past2DayTransactions
}

type Past2DayTransactions {
    customer: CustomerRelationshipAccount
    accounts: [AccountDepositAchTransactions]
}

type AggregatedTransactions {
    mpTransactions: [PendingTransaction]
    achTransactions: [AccountDepositAchTransactions]
}

type LatestTransactionRecord {
    customerDetails: CustomerRelationshipAccount
    accounts: [AccountInfo]
}

type CustomerDetails {
    customerId: String
    accounts: [AccountInfo]
}

type AccountInfo {
    accountId: String
    balance: String
    latestTransaction: AccountDepositAchTransactions
}

type CustomerLatestTransaction {
    customerDetails: CustomerRelationshipAccount
    accounts: AccountDetails
}

type AccountDetails {
    accountId: String
    balance: String
    latestTransaction: AccountDepositAchTransactions
}

type TransactionHistory {
    accountId:String
    pendingTransactions: [PendingTransaction]
    accountDepositAchTransactions: [AccountDepositAchTransactions]
}

type CustomerAccountInfo {
    customerRelationshipAccount: CustomerRelationshipAccount
    pendingTransactions: [PendingTransaction]
    accountDepositAchTransactions: [AccountDepositAchTransactions]
}

