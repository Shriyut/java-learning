    transactionAmount: String
    cashAmount: String
    checkAmount: String
    feeAmount: String
    description: String
    availableBalanceCurrency: String
    availableBalanceAmount: String
    additionalFundingUsedIndicator: Boolean
    originalDepositAmount: String
    sourceSystemReference: String
    sourceTransactionCode: String
    sourceTransactionReference: String
    sourceSequenceNumber: String
    sourceTransactionDate: String
    sourceTransactionTime: String
    transactionRecordedTimestamp: String
    isoTransactionCode: String
    transactionCity: String
    transactionState: String
    transactionAuthorizationCode: String
    pendingPenaltyFeeAmount: String
    carryoverIndicator: Boolean
    additionalFundingUsedAmount: String
    loginType: String
